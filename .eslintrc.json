{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "globals": {
      "window": true,
      "localStorage": true,
      "document": true
    },
    "rules": {
      "react/prop-types": 0,
      "max-len": 0,
      "global-require": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-props-no-spreading": 0,
      "dot-notation": 0,
      "import/prefer-default-export": "off",
      "react/jsx-one-expression-per-line": [
        0,
        {
          "allow": "none"
        }
      ],
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1,
          "VariableDeclarator": 1
        }
      ],
      "no-underscore-dangle": 0,
      "react/jsx-indent": [
        "error",
        4
      ],
      "react/jsx-indent-props": [
        "error",
        4
      ]
    }
  }